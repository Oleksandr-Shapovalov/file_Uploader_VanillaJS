ll.25s ease-in-out;\r\n  cursor: pointer;\r\n  margin-right: 1rem;\r\n}\r\n.btn.primary {\r\n  background: var(--btnColor);\r\n  color: var(--white);\r\n  animation: appear 0.35s ease-out forwards;\r\n}\r\n.btn:active {\r\n  box-shadow: inset 1px 1px 1px rgb(0 0 0 / 30%);\r\n}\r\n.btn:hover {\r\n  opacity: 0.8;\r\n}\r\n.btn.removing {\r\n  animation: remove 0.35s ease-out forwards;\r\n}\r\ninput[type=\"file\"] {\r\n  display: none;\r\n}\r\n.preview {\r\n  transition: all 0.3s ease-in-out;\r\n  margin-top: 2rem;\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  padding: 0.5rem;\r\n  gap: 0.5rem;\r\n  min-height: 6rem;\r\n  background: var(--dropZone);\r\n  border-radius: 1rem;\r\n  border: dashed 2px var(--gray);\r\n}\r\n.preview.active {\r\n  transform: scale(1.02);\r\n  border: solid 2px var(--gray);\r\n}\r\n.preview__image {\r\n  position: relative;\r\n  flex: 1 0 24%;\r\n  overflow: hidden;\r\n  animation: appear 0.35s ease-out forwards;\r\n}\r\n\r\n@media (max-width: 800px) {\r\n  .preview__image {\r\n    flex: 1 0 32.5%;\r\n  }\r\n}\r\n@media (max-width: 500px) {\r\n  .preview__image {\r\n    flex: 1 0 100%;\r\n  }\r\n}\r\n.preview__image img {\r\n  width: 100%;\r\n  height: auto;\r\n  display: block;\r\n}\r\n.preview__image:hover .preview__remove {\r\n  opacity: 1;\r\n}\r\n.preview__image:hover .preview__info {\r\n  transform: translateY(0);\r\n}\r\n.preview__image.removing {\r\n  animation: remove 0.35s ease-out forwards;\r\n}\r\n.preview__remove {\r\n  color: var(--black);\r\n  font-size: 2rem;\r\n  padding: 0 1.5px 3px;\r\n  width: 25px;\r\n  height: 25px;\r\n  position: absolute;\r\n  right: 0;\r\n  top: 0;\r\n  font-weight: 300;\r\n  background: rgba(255, 255, 255, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  cursor: pointer;\r\n  opacity: 0;\r\n  transition: opacity 0.25s ease-in-out;\r\n}\r\n.preview__info {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  color: var(--black);\r\n  font-size: 1.2rem;\r\n  height: 30px;\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  transform: translateY(100%);\r\n  font-weight: 300;\r\n  background: rgba(255, 255, 255, 0.8);\r\n  padding: 0 5px;\r\n\r\n  transition: all 0.25s ease-in-out;\r\n  z-index: 10;\r\n}\r\n.preview__info > span {\r\n  overflow: hidden;\r\n\r\n  white-space: pre;\r\n}\r\n.preview__info > span:first-child {\r\n  text-overflow: ellipsis;\r\n  max-width: 70%;\r\n}\r\n.preview__info_progress {\r\n  position: absolute;\r\n  top: 0;\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  background: var(--btnColor);\r\n  transition: width 0.25s linear;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n@keyframes appear {\r\n  0% {\r\n    transform: scale(0);\r\n  }\r\n  70% {\r\n    transform: scale(1.2);\r\n  }\r\n  100% {\r\n    transform: scale(1);\r\n  }\r\n}\r\n@keyframes remove {\r\n  0% {\r\n    transform: scale(1);\r\n  }\r\n  30% {\r\n    transform: scale(1.2);\r\n  }\r\n  100% {\r\n    transform: scale(0);\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"4c61b24e1eac53b9d793c700def46539","cacheData":{"env":{}}}